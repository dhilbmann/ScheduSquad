<!DOCTYPE html>
<html lang="en" style="position: relative; min-height: 100%;">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewData["Title"] - ScheduSquad.Web</title>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/StaticFiles/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/StaticFiles/css/site.css" asp-append-version="true" />
    <link rel="icon" type="image/x-icon" href="~/StaticFiles/favicon.ico">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="~/StaticFiles/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/StaticFiles/js/site.js"></script>
</head>

<body style="margin-bottom: 60px;">
    <header>

    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <partial name="_Footer.cshtml">

    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>