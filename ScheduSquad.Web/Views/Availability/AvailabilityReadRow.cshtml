@model ScheduSquad.Models.Availability
<div id=@Model.Id>
<tr role="row" data-availability-id=@Model.DayOfWeek>
    <td role="cell"> @Model.DayOfWeek</td>
    <td role="cell"> @Model.StartTime</td>
    <td role="cell"> @Model.EndTime</td>
        <td role="cell"><button class="btn btn-primary edit-button" data-availability-id=@Model.Id>Edit</button></td>
    <td role="cell"><button class="btn btn-primary" type="button" id="deleteButton">Delete</button></td>
</tr>
</div>

